-- Scripts:

local function MAJMX_fake_script() -- CF1.TeleportToCoins 
	local script = Instance.new('LocalScript', CF1)

	local Players = game:GetService("Players")
	local TweenService = game:GetService("TweenService")
	local LocalPlayer = Players.LocalPlayer
	local button = script.Parent
	
	local isTeleporting = false
	
	local function teleportToPart(part)
	    local character = LocalPlayer.Character
	    if character and character.PrimaryPart then
	        local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear)
	        local goal = {CFrame = part.CFrame}
	        local tween = TweenService:Create(character.PrimaryPart, tweenInfo, goal)
	        tween:Play()
	        tween.Completed:Wait()
	    end
	end
	
	local function checkProximityAndTeleport(part)
	    local character = LocalPlayer.Character
	    if character and character.PrimaryPart then
	        local distance = (character.PrimaryPart.Position - part.Position).Magnitude
	        if distance < 10 then
	            character:SetPrimaryPartCFrame(part.CFrame)
	        end
	    end
	end
	
	button.MouseButton1Click:Connect(function()
	    if isTeleporting then return end
	    isTeleporting = true
	
	    while isTeleporting do
	        local success, errorMessage = pcall(function()
	            local coinContainer
	            for _, descendant in ipairs(game.Workspace:GetDescendants()) do
	                if descendant:IsA("Model") and descendant.Name == "CoinContainer" then
	                    coinContainer = descendant
	                    break
	                end
	            end
	
	            if coinContainer then
	                for _, child in ipairs(coinContainer:GetChildren()) do
	                    if child:IsA("BasePart") and child.Parent then
	                        teleportToPart(child)
	                        checkProximityAndTeleport(child)
	                        task.wait(1.6)
	                    end
	                end
	            else
	                task.wait(0.5)
	            end
	        end)
	
	        if not success then
	            warn("Error during teleportation: " .. errorMessage)
	            isTeleporting = false
	        end
	    end
	end)
	
	button.MouseButton2Click:Connect(function()
	    isTeleporting = false
	end)
	
	
end
coroutine.wrap(MAJMX_fake_script)()
local function VVMBMPE_fake_script() -- CF2DetectedDuetoAntiCheat.TeleportToCoinContainer 
	local script = Instance.new('LocalScript', CF2DetectedDuetoAntiCheat)

	local Players = game:GetService("Players")
	local WS = game:GetService("Workspace")
	local TweenService = game:GetService("TweenService")
	local RunService = game:GetService("RunService")
	local LocalPlayer = Players.LocalPlayer
	local button = script.Parent
	
	local function teleportToCoins()
		local coinContainer
	
		-- Wait until CoinContainer is found
		while not coinContainer do
			for _, descendant in WS:GetDescendants() do
				if descendant:IsA("Model") and descendant.Name == "CoinContainer" then
					coinContainer = descendant
					break
				end
			end
			if not coinContainer then
				warn("CoinContainer not found, waiting...")
				task.wait(1)
			end
		end
	
		local function teleportToNextCoin()
			for _, coin in coinContainer:GetChildren() do
				if coin.Name == "Coin_Server" then
					local character = LocalPlayer.Character
					if character then
						local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
						if humanoidRootPart then
							local targetPosition = coin.Position + coin.CFrame.LookVector * 1 -- Teleport in front of the coin
							local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
							local goal = {CFrame = CFrame.new(targetPosition)}
							local tween = TweenService:Create(humanoidRootPart, tweenInfo, goal)
	
							-- Change tween time to 0.5 if not at destination
							local originalTime = tweenInfo.Time
							if (humanoidRootPart.Position - targetPosition).Magnitude > 1 then
								tweenInfo = TweenInfo.new(0.01, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
							end
	
							-- Make player go through any part/basepart
							for _, part in ipairs(character:GetDescendants()) do
								if part:IsA("BasePart") then
									part.CanCollide = true
								end
							end
	
							tween:Play()
							tween.Completed:Wait()
	
							-- Reset tween time back to normal
							tweenInfo = TweenInfo.new(originalTime, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
						end
					end
					task.wait(1)
				end
			end
		end
	
		while coinContainer.Parent do
			RunService.RenderStepped:Wait()
			teleportToNextCoin()
			task.wait(1.5)
		end
	end
	
	button.MouseButton1Click:Connect(teleportToCoins)
	
	
end
coroutine.wrap(VVMBMPE_fake_script)()
local function WQTEW_fake_script() -- CF3.TeleportToCoinParts 
	local script = Instance.new('LocalScript', CF3)

	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer
	local button = script.Parent
	local WS = game:GetService("Workspace")
	
	local function teleportToCFrame(cframe)
		local character = LocalPlayer.Character
		if character then
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart then
				humanoidRootPart.CFrame = cframe
			end
		end
	end
	
	local function FindDescendant()
		while true do
			local CoinContainer = nil
			for _, descendant in WS:GetDescendants() do
				if descendant.Name == "CoinContainer" then
					CoinContainer = descendant
					break
				end
			end
			
			if CoinContainer then
				task.wait(1) -- Wait for 1 second if CoinContainer is found
				local Children = CoinContainer:GetChildren()
				for _, Child in Children do
					if Child:IsA("BasePart") and (Child:FindFirstChild("TouchTransmitter") or Child:FindFirstChild("TouchInterest")) then
						task.wait(0.4)
						teleportToCFrame(Child.CFrame + Vector3.new(0, 5, 0)) -- Teleport slightly above the part
						task.wait(1.5)
						teleportToCFrame(CFrame.new(-145.783, 160.56, 49.015))
						task.wait(1)
					end
				end
			else
				warn("CoinContainer not found, waiting...")
				task.wait(1) -- Wait for 1 second if CoinContainer is not found
				repeat
					task.wait(1)
					for _, descendant in WS:GetDescendants() do
						if descendant.Name == "CoinContainer" then
							CoinContainer = descendant
							break
						end
					end
				until CoinContainer
			end
		end
	end
	
	button.MouseButton1Click:Connect(FindDescendant)
	
	
end
coroutine.wrap(WQTEW_fake_script)()
local function MBQGD_fake_script() -- Frame.Drag 
	local script = Instance.new('LocalScript', Frame)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(MBQGD_fake_script)()
